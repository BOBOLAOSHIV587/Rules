[Script]
# > Github私有仓库
Github Private = type=http-request, requires-body=0, pattern=^https?:\/\/(raw|gist)\.githubusercontent\.com\/, script-path=https://raw.githubusercontent.com/BOBOLAOSHIV587/Rules/main/JS/Github_Private/JS/JS-1/github-private-repo.js


[MITM]
hostname = %APPEND% raw.githubusercontent.com, gist.githubusercontent.com

