[Script]
# > 自动获取Revenuecat核心数据
Revenuecat = type=http-response, pattern=^https:\/\/(api\.revenuecat|isi\.csan\.goodnotes)\.com\/.+\/(receipts$|subscribers\/?(.*?)*$), script-path=https://raw.githubusercontent.com/BOBOLAOSHIV587/Rules/main/JS/Revenuecat/JS/Revenuecat.js, requires-body=true, max-size=-1, timeout=60
Revenuecat = type=http-request, pattern=^https:\/\/(api\.revenuecat|isi\.csan\.goodnotes)\.com\/.+\/(receipts$|subscribers\/?(.*?)*$), script-path=https://raw.githubusercontent.com/BOBOLAOSHIV587/Rules/main/JS/Revenuecat/JS/Revenuecat.js, timeout=60


[MITM]
hostname = %APPEND% api.revenuecat.com, isi.csan.goodnotes.com

