#!name=Troubleshoot
#!desc=故障排除 访问 http://trouble.shoot 查看
#!author=@BOBOLAOSHIV587

[General]
force-http-engine-hosts = %APPEND% trouble.shoot

[MITM]
hostname = %APPEND% trouble.shoot, *.trouble.shoot

[Script]
troubleshoot = type=http-request, pattern=^https?:\/\/trouble\.shoot, script-path=https://raw.githubusercontent.com/BOBOLAOSHIV587/Rules/main/JS/Network-Info/JS/troubleshoot.js, timeout=300
