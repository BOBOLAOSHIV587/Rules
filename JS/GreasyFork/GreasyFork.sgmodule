#!name=Greasyfork油猴脚本转换器
#!desc= 打开一个油猴脚本安装页面，点击Install，此时应该会弹出通知提醒脚本已经转换成功。将转换好脚本移动至APP对应目录，根据生成的脚本提示设置好重写即可。
#!date=2025-08-20
#!category=😍BOBO Premium
#!author= Peng-YM[https://github.com/Peng-YM]
#!icon=https://raw.githubusercontent.com/BOBOLAOSHIV587/Rules/main/Icons/AppIcons/Image/App/Tampermonkey.png
#!openUrl=https://apps.apple.com/app/id6738342400


[Script]
# > 油猴脚本转换器
油猴脚本转换器=type=http-response, pattern=^https:\/\/(greasyfork|openuserjs)\.org\/.*\/.*\.user\.js, script-path=https://raw.githubusercontent.com/BOBOLAOSHIV587/Rules/main/JS/GreasyFork/JS/GreasyFork.js, requires-body=true


[MITM]
hostname = %APPEND% greasyfork.org, openuserjs.org

